{
  "name": "markdown-it-comment",
  "version": "2.0.1",
  "packageManager": "pnpm@7.11.0",
  "description": "typescript-template",
  "author": "peterroe",
  "license": "MIT",
  "keywords": [
    "typescript",
    "template"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "esbuild --bundle --format=cjs  src/index.ts --outfile=dist/index.cjs && esbuild --bundle --format=esm  src/index.ts --outfile=dist/index.mjs",
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "eslint --fix \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "release": "bumpp",
    "docs:build": "vite build && gh-pages -d docs",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.26.3",
    "@types/markdown-it": "^12.2.3",
    "bumpp": "8.2.1",
    "esbuild": "^0.15.9",
    "eslint": "8.24.0",
    "esno": "0.16.3",
    "markdown-it": "^13.0.1",
    "typescript": "4.8.3",
    "vite": "3.1.1",
    "vitest": "0.23.4"
  }
}
